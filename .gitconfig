[alias]
  br = branch
  ci = commit
  co = checkout
  df = diff
  pb = publish-branch
  st = status
  ms = merge --squash
  pr = pull --rebase
  prum = pull --rebase upstream master
  prom = pull --rebase origin master
  pfu = push -f upstream
  pfo = push -f origin
  rmbr = "![ x$# != x1 ]&&echo "branch required" >&2 || git co master && git prum && git co \"$1\" && git prum && git co master && git br -d \"$1\" #"
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[merge]
  log = true
[difftool "Kaleidoscope"]
  cmd = ksdiff-wrapper git \"$LOCAL\" \"$REMOTE\"
[rerere]
  enabled = 1
[url "git@github.com:"]
  insteadOf = "gh:"
  pushInsteadOf = "github:"
  pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
  insteadOf = "github:"
[core]
  editor = mvim -f --nomru -c \"au VimLeave * !open -a iTerm2\"
  excludesfile = ~/.gitignore_global
[user]
  name = Eddie Monge
; email = emailaddresshere
; signingkey = keysignaturehere
[credential]
  helper = osxkeychain
[push]
  default = current
; [remote "upstream"]
;   fetch = +refs/pull/*/head:refs/remotes/upstream/pr/*
[commit]
  gpgsign = true
[rebase]
	autosquash = true
