# Dir navigation
alias ..='cd ..'
alias ~='cd ~'
alias ll="ls -alh"
alias web="cd /Users/eddie/Sites"


# Utilities
alias g="git"
alias vim="mvim"
alias vimdiff="mvim -d"
alias startvm="VBoxManage startvm --type headless"
alias vbox="VBoxManage controlvm"
alias hgstash="hg diff > stash.diff;hg revert --no-backup --all;hg pullr;patch -p1 < stash.diff;rm stash.diff"
alias wget="curl -O"


# Development stuff
alias edithosts="sudo mvim /etc/hosts"
alias gitsync="git pull upstream master && git push origin master"
